{
  "compilerOptions": {
    "target": "ES6", // 또는 ESNext
    "lib": ["es2017", "dom"],
    "module": "ESNext",
    "jsx": "react-jsx",
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true
  },
  "include": ["src/**/*"], // 프로젝트에 포함될 경로
  "exclude": ["node_modules"] // 제외할 경로
}
